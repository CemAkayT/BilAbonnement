CREATE TABLE `fejl` (
  `fejl_id` int NOT NULL AUTO_INCREMENT,
  `fejl_type` varchar(45) NOT NULL,
  `fejl_pris` double NOT NULL,
  `bil_ID` int NOT NULL,
  PRIMARY KEY (`fejl_id`),
  UNIQUE KEY `fejl_id_UNIQUE` (`fejl_id`),
  UNIQUE KEY `bil_ID_UNIQUE` (`bil_ID`),
  CONSTRAINT `fejl_ibfk_1` FOREIGN KEY (`bil_ID`) REFERENCES `bil` (`bil_ID`)
  );