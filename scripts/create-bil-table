CREATE TABLE `bil` (
  `årgang` int NOT NULL,
  `mærke` varchar(45) NOT NULL,
  `model` varchar(45) NOT NULL,
  `farve` varchar(45) NOT NULL,
  `bil_ID` int NOT NULL AUTO_INCREMENT,
  `kundeID` int DEFAULT NULL,
  `pris` double NOT NULL,
  `fejl` varchar(255) DEFAULT NULL,
  `fejl_pris` double DEFAULT NULL,
  PRIMARY KEY (`bil_ID`),
  UNIQUE KEY `id_UNIQUE` (`bil_ID`)
);